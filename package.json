{
  "name": "midi-song-creation-tool",
  "version": "1.0.0",
  "description": "MIDI Song Creation Tool - An agentic framework for creating and manipulating MIDI music",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --selectProjects=server,client,core",
    "test:integration": "jest tests/integration",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:ci": "jest --ci --coverage && cypress run",
    "start": "node src/server/server.js",
    "dev": "nodemon src/server/server.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^7.5.0",
    "body-parser": "^1.20.2",
    "multer": "^1.4.5-lts.1",
    "uuid": "^9.0.0",
    "mongodb-memory-server": "^8.15.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "cypress": "^13.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "msw": "^1.3.2",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3"
  }
}