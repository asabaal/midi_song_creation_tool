{
  "name": "midi-song-creation-tool",
  "version": "1.0.0",
  "description": "MIDI Song Creation Tool - An agentic framework for creating and manipulating MIDI music",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --selectProjects=server,client,core",
    "test:integration": "cross-env NODE_ENV=test jest tests/integration",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:ci": "jest --ci --coverage && cypress run",
    "test:all": "bash scripts/run-tests.sh",
    "test:coverage:view": "open coverage/lcov-report/index.html || start coverage/lcov-report/index.html || xdg-open coverage/lcov-report/index.html",
    "test:report": "open scripts/coverage-viewer.html || start scripts/coverage-viewer.html || xdg-open scripts/coverage-viewer.html",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "node src/server/server.js",
    "dev": "nodemon src/server/server.js",
    "prepare": "chmod +x scripts/run-tests.sh",
    "generate:fixtures": "node scripts/generate-test-midi.js"
  },
  "dependencies": {
    "body-parser": "^1.20.2",
    "express": "^4.18.2",
    "mongoose": "^7.5.0",
    "multer": "^1.4.5-lts.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "cross-env": "^7.0.3",
    "cypress": "^13.3.0",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-cypress": "^2.15.2",
    "eslint-plugin-jest": "^27.4.2",
    "eslint-plugin-prettier": "^5.2.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-junit": "^16.0.0",
    "jest-watch-typeahead": "^2.2.2",
    "msw": "^1.3.2",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "supertest": "^6.3.3"
  },
  "jest-junit": {
    "outputDirectory": "./test-results/jest",
    "outputName": "results.xml",
    "ancestorSeparator": " â€º ",
    "uniqueOutputName": "false",
    "suiteNameTemplate": "{filepath}",
    "classNameTemplate": "{classname}",
    "titleTemplate": "{title}"
  }
}
